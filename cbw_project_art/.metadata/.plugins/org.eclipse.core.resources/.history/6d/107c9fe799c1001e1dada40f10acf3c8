package com.cbw.art.dto;

import com.cbw.art.model.Purchase;

public class PurchseDto {
	
	private Long lectureId;
	private Long userId;
	
	public PurchseDto() {
		super();
	}

	public PurchseDto(Long lectureId, Long userId) {
		super();
		this.lectureId = lectureId;
		this.userId = userId;
	}

	public Long getLectureId() {
		return lectureId;
	}

	public void setLectureId(Long lectureId) {
		this.lectureId = lectureId;
	}

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}
	
	public static PurchseDto from(Purchase purchase) {
		if(purchase == null) return null;
		
	}
}
