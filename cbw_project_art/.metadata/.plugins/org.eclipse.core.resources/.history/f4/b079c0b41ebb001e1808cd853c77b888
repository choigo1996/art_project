package com.cbw.art.dto;

import java.util.List;

import com.cbw.art.enumstatus.CategoryType;

public class CategoryDto {
	
	private CategoryType categoryTypes;
	private Long lectureId;
	
	public CategoryDto() {
		super();
	}

	public CategoryDto(CategoryType categoryTypes, Long lectureId) {
		super();
		this.categoryTypes = categoryTypes;
		this.lectureId = lectureId;
	}

	public CategoryType getCategoryTypes() {
		return categoryTypes;
	}

	public void setCategoryTypes(CategoryType categoryTypes) {
		this.categoryTypes = categoryTypes;
	}

	public Long getLectureId() {
		return lectureId;
	}

	public void setLectureId(Long lectureId) {
		this.lectureId = lectureId;
	}

	
}
